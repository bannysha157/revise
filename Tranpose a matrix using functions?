              
#include <stdio.h>
void print(int [3][3]); // function declaration
int main() {
 int a[3][3],i,j;
 printf("enter elements");
 for(i=0;i<3;i++)
 for(j=0;j<3;j++)
 scanf("%d" ,&a[i][j]);
 print(a);
 return 0;
}
void print(int a[3][3]) // function defination
{
    int i,j;
    printf("\n after Tranpose the matrix");
    for(i=0;i<3;i++)
    {
 for(j=0;j<3;j++)
 {
 printf("%d" ,a[j][i]);
}
    printf("\n");
    }
}
