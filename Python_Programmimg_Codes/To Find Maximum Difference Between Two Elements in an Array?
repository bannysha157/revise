X = [[1, 3], [4,5], [7, 9]]
result = [0, 0, 0], [0, 0, 0]]
for i in range (len (X)):
   for j in range (len (X[0])):
 result [j][i] = X [i][j]
for r in result:
 print (r)
 
 



